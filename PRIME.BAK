};
void prime::get()
{
	cout<<"enter the prime number"<<endl;
	cin>>n;
}

	for(n=2;n<=20;n++)
	{
		f=1;
		for(i=2;i<=n-1;i++)
			{
				if(n%i==0)
					{
						f=0;
					}
			 }
	}
	if(f==0)
	{
		cout<<"\n number "<<n<<"is not a prime number"<<endl;
	}
	else
	{
		cout<<"\n number"<<n<<"is prime number"<<endl;
	}
